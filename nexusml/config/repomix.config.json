{
  "output": {
    "filePath": "nexusml-repomix-output.md",
    "style": "markdown",
    "parsableStyle": true,
    "fileSummary": true,
    "directoryStructure": true,
    "removeComments": false,
    "removeEmptyLines": false,
    "compress": true,
    "topFilesLength": 50,
    "showLineNumbers": false,
    "copyToClipboard": true
  },
  "include": ["nexusml/"],
  "ignore": {
    "useGitignore": true,
    "useDefaultPatterns": true,
    "customPatterns": [
      "nexusml/ingest/**",
      "nexusml/docs",
      "nexusml/output/**",
      "nexusml/core/deprecated/**",
      "nexusml/tests/**",
      "nexusml/ingest/**",
      "nexusml/examples/**",
      "nexusml/notebooks/configuration_example.ipynb ",
      "nexusml/notebooks/configuration_guide.md",
      "nexusml/notebooks/configuration_examples.py",
      "nexusml/notebooks/**",
      "nexusml/test_output/**",
      "nexusml/readme.md",
      "nexusml/requirements.txt",
      "nexusml/setup.py",
      "nexusml/setup.cfg",
      "nexusml/pyproject.toml",
      "nexusml/tox.ini"
      
    ]
  },
  "security": {
    "enableSecurityCheck": true
  },
  "tokenCount": {
    "encoding": "o200k_base"
  }
}
